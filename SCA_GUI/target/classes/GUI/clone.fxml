<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="cloneAnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CloneController">
   <children>
      <Button fx:id="cloneBackButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickOnCloneBackButton" text="Back" />
      <Label fx:id="cloneLabel1" layoutX="212.0" layoutY="54.0" prefHeight="37.0" prefWidth="177.0" text="Select a directory   " textAlignment="CENTER">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </Label>
      <Label fx:id="cloneLabel2" layoutX="142.0" layoutY="91.0" text="Your directory musn't contain more than 5 files" />
      <Button fx:id="cloneBrowseButton" layoutX="440.0" layoutY="176.0" mnemonicParsing="false" onAction="#clickOnBrowseButton" text="Browse" />
      <TextField fx:id="cloneTextField" layoutX="201.0" layoutY="176.0" prefHeight="24.0" prefWidth="199.0" />
      <Button fx:id="detectCloneButton" layoutX="249.0" layoutY="279.0" mnemonicParsing="false" onAction="#clickOnDetectClone" text="Detect Clone" />
   </children>
</AnchorPane>
