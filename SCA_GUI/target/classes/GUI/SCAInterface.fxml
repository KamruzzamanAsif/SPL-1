<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="SCAInterface" prefHeight="400.0" prefWidth="600.0" styleClass="root" stylesheets="@../CSS/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.SCAInterfaceController">
   <children>
    <Button fx:id="SCAInterfaceBackButton" layoutX="14.0" layoutY="14.0" minWidth="51.0" mnemonicParsing="false" onAction="#clickOnSCAInterfaceBackButton" prefHeight="24.0" prefWidth="57.0" stylesheets="@../CSS/button.css" text="Back" textFill="#194086">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button fx:id="MetricsButton" layoutX="119.0" layoutY="280.0" mnemonicParsing="false" onAction="#clickOnMetricsButton" prefHeight="0.0" prefWidth="132.0" stylesheets="@../CSS/button.css" text="Metrics">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <Button fx:id="CloneButton" layoutX="344.0" layoutY="280.0" mnemonicParsing="false" onAction="#clickOnCloneButton" prefHeight="0.0" prefWidth="150.0" text="Clone">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Button>
      <ImageView id="metricsIcon" fx:id="metricsIcon" fitHeight="150.0" fitWidth="200.0" layoutX="110.0" layoutY="115.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/metrics.png" />
         </image>
         <effect>
            <Blend />
         </effect>
      </ImageView>
      <ImageView fx:id="cloneIcon" fitHeight="150.0" fitWidth="200.0" layoutX="344.0" layoutY="115.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/clone.png" />
         </image>
         <effect>
            <Blend />
         </effect>
      </ImageView>
   </children>
</AnchorPane>
